program diferite;
var
	c1,c2: char;
   n1,n2: integer;
begin
	writeln('Intoduceti in ordine: caracter numar caracter numar');
   readln(c1,n1,c2,c2,n2); { varianta corecta nu cu un singur c2 }
   writeln('Caracter : "',c1,'"');
   writeln('Numar : ', n1);
   writeln('Caracter : "',c2,'"');
   writeln('Numar : ', n2);
   write('Pentru continuare apasati ENTER');
   readln;
end.